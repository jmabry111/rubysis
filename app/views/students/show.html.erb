<div class="center hero-unit">
	<div class="row">
		<div class="span9">
			<%= @student.to_s %>
		</div>
	</div>
	<% if @student.addresses.any? %>
		<%= render @addresses %>
	<% end %>
	<% if @student.parents.any? %>
		<%= render @parents %>
	<% end %>
	<%= link_to "Student Path", student_path(@student) %>
</div>